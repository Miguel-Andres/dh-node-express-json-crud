<%- include('../partials/head') %>
    <h1>Listado de productos</h1>
    <section class="products">
        <% products.forEach(product => { %>
            <article class="product">
                <h3><%= product.title %></h3>
                <p class="desc"><%= product.desc %></p>
                <p class="price">$ <%= product.price %></p>
            </article>
        <% }); %>
    </section>
    
<%- include('../partials/footer') %>